[32m+[m[32m        private void button15_Click(object sender, EventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            listView1.Items.Add("hdfh");[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                if (textBox1.Text == "")[m
[32m+[m[32m                {[m
[32m+[m[32m                    textBox1.Text = buffer;[m
[32m+[m[41m                    [m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[41m                             [m
[32m+[m[32m                    if (CurrentOperation == operation.addiction)[m
[32m+[m[32m                    {[m
[32m+[m[41m                      [m
[32m+[m[32m                        double tmpBuffer = double.Parse(textBox1.Text);[m
[32m+[m[32m                        textBox1.Text = (double.Parse(buffer) + tmpBuffer).ToString();[m
[32m+[m[32m                        CurrentOperation = operation.clearOperation;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    if (CurrentOperation == operation.substraction)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        double tmpBuffer = double.Parse(textBox1.Text);[m
[32m+[m[32m                        textBox1.Text = (double.Parse(buffer) - tmpBuffer).ToString();[m
[32m+[m[32m                        CurrentOperation = operation.clearOperation;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    if (CurrentOperation == operation.multiplication)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        double tmpBuffer = double.Parse(textBox1.Text);[m
[32m+[m[32m                        textBox1.Text = (double.Parse(buffer) * tmpBuffer).ToString();[m
[32m+[m[32m                        CurrentOperation = operation.clearOperation;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    if (CurrentOperation == operation.division)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        double tmpBuffer = double.Parse(textBox1.Text);[m
[32m+[m[32m                        textBox1.Text = (double.Parse(buffer) / tmpBuffer).ToString();[m
[32m+[m[32m                        CurrentOperation = operation.clearOperation;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[41m            [m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show("Try input incorrect data");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        } // result button[m
[32m+[m
[32m+[m[32m        private void button17_Click(object sender, EventArgs e)[m
